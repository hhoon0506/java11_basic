package org.kh.site2;
//대입 연산자 :연산 후에 대입하는 연산자 +=,-=,*=,/=,%=,&=,|=,>>=,<<=,>>>=,^=
public class Operation7 {

	public static void main(String[] args) {
		int a = 20;
		int b= 15;
		int c;
		a += b;
		b -= a;
		c = a;
		a = b;
		b = c;
		
		System.out.println();

		int x =0b01101;
		int y = 0b01011;
		System.out.println(x);
		System.out.println(Integer.toBinaryString(x & y));
		
	}

}
