package org.kh.java;

public class Variable5 {

	public static void main(String[] args) {
		char ch1 = 'H';
		char ch2 = 65;
		char ch3 = 82;
		char ch4 = '\u2665';
		System.out.println("ch1 : "+ch1);
		System.out.println("코드 넘버(번호)");
		System.out.println("ch1 : "+(int)ch1);
		System.out.println("ch4 : "+(int)ch4);
	}

}
